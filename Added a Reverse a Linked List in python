class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

class LinkedList:
    def __init__(self):
        self.head = None

    def reverse(self, node):
        prev = None
        current = node

        while current:
            next_node = current.next
            current.next = prev
            prev = current
            current = next_node

        node = prev
        return node

    def print_list(self, node):
        while node:
            print(node.data, end=" ")
            node = node.next
        print()

# Driver Code
if __name__ == "__main__":
    list = LinkedList()
    list.head = Node(85)
    list.head.next = Node(15)
    list.head.next.next = Node(4)
    list.head.next.next.next = Node(20)

    print("Given linked list")
    list.print_list(list.head)
    list.head = list.reverse(list.head)
    print("Reversed linked list")
    list.print_list(list.head)
